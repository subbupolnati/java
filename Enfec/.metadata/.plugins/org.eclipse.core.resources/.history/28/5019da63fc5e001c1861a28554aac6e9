package com.string;
import java.util.*;
public class Vowels {
	public static void main(String[] args) {
		int count=0;
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter String");
		String str=sc.next();
		for(int i=0;i<str.length();i++) {
			
			if(str.charAt(i)=='a' || str.charAt(i)=='A' || str.charAt(i)=='e')
			{
				System.out.print(count);
				count=count+1;
			}
			
		}
		
	}

}
