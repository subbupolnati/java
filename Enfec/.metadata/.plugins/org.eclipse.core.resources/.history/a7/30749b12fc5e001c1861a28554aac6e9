package com.string;
import java.util.*;
public class Vowels {

	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter String");
		String str=sc.next();
		for(int i=0;i<=str.length();i++) {
			int count=0;
			if(str.charAt(i)=='a' && str.charAt(i)=='A') {
			count=count+1;
			}
			System.out.print(count);
		}

	}

}
